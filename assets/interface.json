                    "name": "ImportMinimumProfit",
                    "label": "$option.ImportMinimumProfit.inputs.ImportMinimumProfit.label",
                    "description": "$option.ImportMinimumProfit.inputs.ImportMinimumProfit.description",
                    "pipeline_type": "int",
                    "verify": "^\\d+$",
